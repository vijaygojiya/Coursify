{
  "name": "coursify",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild",
    "clean": "expo prebuild --clean",
    "clean:android": "npx expo prebuild --clean --platform android",
    "clean:ios": "npx expo prebuild --clean --platform ios",
    "clean:cache": "watchman watch-del-all && yarn cache clean",
    "clean:builds": "rm -rf ./builds && mkdir ./builds",
    "android:release": "expo run:android  --variant release",
    "ios:release": "expo run:ios --configuration Release",
    "pull:dev": "eas env:pull  --environment=development  --non-interactive",
    "android:build:local": "eas build --platform android --profile development --local --non-interactive --no-wait --clear-cache --output ./builds/Coursify.apk",
    "android:submit": "eas submit  --platform android --profile development --non-interactive --no-wait --path ./builds/android-prod.aab",
    "ios:build:local": "eas build --platform ios --profile development --local --non-interactive --no-wait --clear-cache --output ./builds/ios-dev.ipa",
    "ios:submit": "eas submit  --platform ios --profile development --non-interactive --no-wait --path ./builds/ios-dev.ipa",
    "lint": "expo lint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/material-top-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/query-async-storage-persister": "^5.90.12",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-persist-client": "^5.90.12",
    "expo": "54.0.25",
    "expo-build-properties": "~1.0.9",
    "expo-font": "~14.0.9",
    "expo-image-crop-tool": "^0.4.0",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-network": "~8.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-video-thumbnails": "~10.0.7",
    "mime": "^4.1.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bootsplash": "^6.3.11",
    "react-native-fast-collapsible": "^1.1.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-mmkv": "^4.0.0",
    "react-native-nitro-modules": "^0.31.4",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-worklets": "0.5.1",
    "sonner-native": "^0.21.1",
    "tus-js-client": "^4.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.3",
    "react-native-svg-transformer": "^1.5.2",
    "typescript": "~5.9.3"
  },
  "private": true
}
